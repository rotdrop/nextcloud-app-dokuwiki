{
  "name": "rotdrop/dokuwiki-nextcloud",
  "description": "Embed DokuWiki into NextCloud",
  "type": "project",
  "license": "AGPL",
  "authors": [
    {
      "name": "Claus-Justus Heine",
      "email": "himself@claus-justus-heine.de"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "require": {
    "phpxmlrpc/phpxmlrpc": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "nextcloud/coding-standard": "*",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*",
    "zebra-north/phpcs-short-types": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "OCA\\RotDrop\\Toolkit\\": "lib/Toolkit/"
    }
  }
}
